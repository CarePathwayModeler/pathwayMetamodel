<?xml version="1.0" encoding="UTF-8"?>
<pathwayMetamodel:Pathway
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pathwayMetamodel="http://www.example.org/pathwayMetamodel"
    xsi:schemaLocation="http://www.example.org/pathwayMetamodel pathwayMetamodel.ecore">
  <element xsi:type="pathwayMetamodel:AuxiliaryConduct"
      name="Vacinacao"
      outputSequences="//@sequence.0 //@sequence.1 //@sequence.2">
    <questions
        text="Necessita_Vacinacao">
      <answers
          xsi:type="pathwayMetamodel:YesOrNo"
          name="necessita_vacinacao"/>
    </questions>
    <questions
        text="Idade"/>
    <questions
        text="Peso"/>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment"
      name="Grau 1"
      inputSequences="//@sequence.0"
      outputSequences="//@sequence.3">
    <examination
        url="Hemograma Completo"/>
    <medication
        name="Novalgina"/>
    <medication
        name="Dipirona"/>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment"
      name="Grau 2"
      inputSequences="//@sequence.1"
      outputSequences="//@sequence.4">
    <examination
        url="Sumario de Urina"/>
    <examination
        url="Cardiotocografia"/>
  </element>
  <element xsi:type="pathwayMetamodel:Treatment"
      name="Grau 3"
      inputSequences="//@sequence.2"
      outputSequences="//@sequence.5">
    <examination
        url="PotÃ¡ssio"/>
    <procedure
        url="Manobra de Epley"/>
  </element>
  <element xsi:type="pathwayMetamodel:Information"
      name="Info 1"
      inputSequences="//@sequence.3 //@sequence.4"/>
  <element xsi:type="pathwayMetamodel:Information"
      name="Info 2"
      inputSequences="//@sequence.5"/>
  <sequence
      inputStep="//@element.1"
      outputStep="//@element.0"
      name="S1">
    <operation
        name="add &lt; 2"
        Operator="lessThan">
      <operand
          xsi:type="pathwayMetamodel:Operation"
          Operator="addition">
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="A"
            weight="3"/>
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="B"
            weight="2"/>
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="C"
            weight="1"/>
      </operand>
      <operand
          xsi:type="pathwayMetamodel:Numeric"
          name="2"
          value="2.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@element.2"
      outputStep="//@element.0"
      name="S2">
    <operation
        name="add >= 3"
        Operator="greaterOrEqual">
      <operand
          xsi:type="pathwayMetamodel:Operation"
          Operator="addition">
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="A"
            weight="3"/>
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="B"
            weight="2"/>
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="C"
            weight="1"/>
      </operand>
      <operand
          xsi:type="pathwayMetamodel:Numeric"
          name="3"
          value="3.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@element.3"
      outputStep="//@element.0"
      name="S3">
    <operation
        name="add = 3"
        Operator="equal">
      <operand
          xsi:type="pathwayMetamodel:Operation"
          Operator="addition">
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="A"
            weight="3"/>
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="B"
            weight="2"/>
        <operand
            xsi:type="pathwayMetamodel:YesOrNo"
            name="C"
            weight="1"/>
      </operand>
      <operand
          xsi:type="pathwayMetamodel:Numeric"
          name="3"
          value="3.0"/>
    </operation>
  </sequence>
  <sequence
      inputStep="//@element.4"
      outputStep="//@element.1"
      name="S4"/>
  <sequence
      inputStep="//@element.4"
      outputStep="//@element.2"
      name="S5"/>
  <sequence
      inputStep="//@element.5"
      outputStep="//@element.3"
      name="S6"/>
</pathwayMetamodel:Pathway>

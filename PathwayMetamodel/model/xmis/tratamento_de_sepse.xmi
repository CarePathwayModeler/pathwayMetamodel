  <element xsi:type="pathwayMetamodel:Information" id="3802" type="Informacao" type_verbose="Informa&#xe7;&#xe3;o" url="http://app-desenv.hapvida.com.br/api/protocolo/hospitalar/protmed/autoria/protocolos/1182/informacoes/3802/" name="Aviso: Encaminhar paciente ao l&#xed;der local" description="* ATEN&#xc7;&#xc3;O:&#xA;Dar seguimento ao tratamento junto ao l&#xed;der local:&#xA;&#xA;- MONITORIZA&#xc7;&#xc3;O PA + OXIMETRIA DE PULSO (SE PAM&lt; 65 OU LACTATO > 18MG/DL)&#xA;- PASSAR SVD E ACOMPANHAR D&#xc9;BITO URIN&#xc1;RIO (ALVO 0,5ML/KG/H)" isTerminal="true" inputSequences="//@sequence.8 //@sequence.9 //@sequence.10 //@sequence.11 //@sequence.13 //@sequence.14 //@sequence.15"/>
